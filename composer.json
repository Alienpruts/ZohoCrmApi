{
    "name": "webleit/zohocrmapi",
    "description": "Zoho CRM API - PHP SDK",
    "license": "MIT",
    "keywords": ["zoho", "crm", "zohocrm", "api"],
    "authors": [
        {
            "name": "Daniele Rosario",
            "email": "daniele@weble.it"
        }
    ],
    "require": {
        "php" : ">=7.0.0",
        "guzzlehttp/guzzle": "~6.0",
        "tightenco/collect": "^6.0",
        "psr/cache": "^1.0",
        "doctrine/inflector": "^1.3",
        "bentools/guzzle-throttle-middleware": "^0.4.0",
        "weble/zohoclient": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*"
    },
    "autoload": {
        "psr-4": {
            "Webleit\\ZohoCrmApi\\": "src"
        }
    },
    "repositories": [{
        "type": "vcs",
        "url": "git@github.com:weble/zohoclient.git"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true
}
